<div class="container">
  <div class="mx-auto mt-5">
    <h5 class="text-left pb-4">
  	  <span style="background-color:#f5f5f5">　ジャンル編集　</span>
    </h5>
    
    <%= form_with model: @genre, url: admin_genre_path, local: true do |f| %>
      <label for="genreEdit">ジャンル名</label>
      <%= f.text_field :name, id: "genreEdit" %>
      <%= f.submit "変更を保存", class:"btn btn-success" %>
    <% end %>

  </div>
</div>